{
    "name": "@contentrain/core",
    "version": "1.0.0",
    "description": "Core package for Contentrain SDK, providing fundamental functionality and types",
    "author": "Contentrain",
    "license": "MIT",
    "private": false,
    "keywords": [
        "contentrain",
        "sdk",
        "core",
        "content-management"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsup && rm -rf dist/types dist/cache dist/loader dist/query",
        "test": "vitest watch",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "clean": "rm -rf dist"
    },
    "devDependencies": {
        "@types/node": "^20.10.6",
        "@vitest/coverage-v8": "^1.1.3",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3",
        "vitest": "^1.1.3"
    },
    "dependencies": {
        "tiny-lru": "^11.2.11"
    }
}
