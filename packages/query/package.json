    "@vitest/coverage-v8": "^1.0.0",
    "react": "^18.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "vue": "^3.5.13"
